{
  "name": "unreal-contracts",
  "version": "0.1.0",
  "license": "Apache-2.0",
  "private": true,
  "description": "Unreal:gas token for AI Compute",
  "author": "Hiro <laciferin@gmail.com>",
  "resolutions": {
    "ethers": "^5.7.2"
  },
  "scripts": {
    "setup": "remixd -i slither",
    "node": "hardhat node",
    "print-env": "ts-node scripts/generate-addresses.ts",
    "gen": "hardhat new-wallet --network",
    "gen-env": "ts-node scripts/generate-addresses.ts | tee .env.gen",
    "gen-env0": "npm run print-env | grep = > .env.gen",
    "print:env": "ts-node scripts/generate-addresses.ts",
    "print:contract": "hardhat run scripts/print-contract-env.ts --network ",
    "pkey": "node scripts/derive-pkey.js ",
    "dev": "remixd",
    "style": "prettier -w .",
    "start": "hardhat node --network hardhat",
    "prebuild": "hardhat clean",
    "build": "hardhat compile",
    "postbuild": "hardhat typechain",
    "build1": "export TS_NODE_TRANSPILE_ONLY=1 && echo 'run when hardhat fails due to insufficient types' && hardhat compile",
    "clean": "hardhat clean && rm -rf deployments",
    "hardhat:local": "hardhat node",
    "pkey1": "hardhat pkey ",
    "pret": "rimraf deployments/localhost && pnpm run deploy localhost && pnpm fund localhost",
    "t": "pnpm run test --network localhost --bail",
    "test": "CONFIG=./.env hardhat test",
    "d": "pnpm run deploy",
    "deploy": "hardhat deploy --network ",
    "deploy:calibration": "hardhat deploy --network calibration",
    "deploy:sepolia": "rm -rf deployments/sepolia && hardhat deploy --network sepolia && hardhat fund --network sepolia",
    "deploy:chaos": "hardhat deploy --network chaos",
    "skale": "rm -rf deployments/titanAI && pnpm hardhat deploy --network titanAI",
    "postskale": "pnpm dapp titanAI && pnpm fund titanAI 0.0000000001 10000",
    "deploy:titanAI": "hardhat deploy --network titanAI",
    "task:accounts": "hardhat accounts",
    "task:balance": "hardhat balance --network calibration --account",
    "task:bal": "hardhat bal --network calibration",
    "task:networks": "hardhat networks",
    "task:deployAll": "hardhat deployAll --skip chaos,localhost ",
    "task:deployAll:parallel": "hardhat deployAll --parallel --skip haos,localhost ",
    "destroy": "hardhat destroy --network ",
    "scripts:deployAll": "hardhat run scripts/deployAll.ts",
    "fund": "hardhat fund --network ",
    "drip": "hardhat drip --network ",
    "odp": "hardhat odp --network ",
    "unreal": "hardhat unreal --network ",
    "dapp": "hardhat dapp --network ",
    "bal": "hardhat bal --network",
    "balance": "npm run bal",
    "testseries": "hardhat test --grep \"series\" --network titanAI",
    "testparallel": "hardhat test --grep \"parallel\" --network titanAI",
    "eCounter": "echidna contracts/forge/Counter.sol",
    "sourcify": "hardhat sourcify --network",
    "flatten": "forge flatten ",
    "token:approve": "hardhat token:approve --network"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-chai-matchers": "^2.1.0",
    "@nomicfoundation/hardhat-ethers": "^3.1.0",
    "@nomicfoundation/hardhat-ignition": "^0.15.13",
    "@nomicfoundation/hardhat-ignition-ethers": "^0.15.14",
    "@nomicfoundation/hardhat-network-helpers": "^1.1.0",
    "@nomicfoundation/hardhat-toolbox": "^3.0.0",
    "@nomicfoundation/hardhat-verify": "^1.1.1",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@openzeppelin/contracts": "^4.9.6",
    "@openzeppelin/contracts-upgradeable": "^4.9.6",
    "@remix-project/remixd": "^0.6.50",
    "@typechain/ethers-v6": "^0.4.3",
    "@typechain/hardhat": "^8.0.3",
    "@types/bluebird": "^3.5.42",
    "@types/chai": "^4.3.20",
    "@types/chai-as-promised": "^7.1.8",
    "@types/minimist": "^1.2.5",
    "@types/mocha": "^10.0.10",
    "@types/node": "^20.19.9",
    "bluebird": "^3.7.2",
    "chai": "^4.5.0",
    "chai-as-promised": "^7.1.2",
    "concurrently": "^8.2.2",
    "dotenv": "^16.6.1",
    "ethereumjs-wallet": "^1.0.2",
    "ethers": "^6.15.0",
    "hardhat": "^2.26.1",
    "hardhat-deploy": "^0.11.45",
    "hardhat-deploy-ethers": "^0.4.2",
    "hardhat-gas-reporter": "^1.0.10",
    "minimist": "^1.2.8",
    "moment": "^2.30.1",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "solc": "0.8.28",
    "solidity-coverage": "^0.8.16",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.2",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=6.0.0",
    "pnpm": ">=8.6.6"
  },
  "packageManager": "pnpm@9.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/unreal-art/hardhat"
  },
  "bugs": {
    "url": "https://gitlab.com/darts-network/darts/issues"
  },
  "trustedDependencies": [
    "fsevents"
  ]
}